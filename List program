numberList = []
n = int(input("Enter the list size : "))

for i in range(0, n):
    item = int(input())
    numberList.append(item)
print("User List is ", numberList)

print("Even elements are: ")
for i in range(0,n):
    if(numberList[i]%2==0):
        print(numberList[i])

print("Odd elements are: ")
for i in range(0,n):
    if(numberList[i]%2!=0):
        print(numberList[i])
